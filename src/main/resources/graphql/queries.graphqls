type Query {
    greeting: Greeting
    customers: [Customer]
    customerById(id: ID): Customer
    hello: String
    helloWithName(name: String) : String
    customerByName(name: String): Customer
}

type Customer {
    id: ID
    name: String
    profile: Profile
}

type Profile {
    id: ID
    customerId: ID
}

type Greeting {
    greeting : String
}